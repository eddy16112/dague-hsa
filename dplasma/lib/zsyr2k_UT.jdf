descA     [type = "tiled_matrix_desc_t" hidden = on default = "*((tiled_matrix_desc_t*)dataA)"]
dataB     [type = "dague_ddesc_t *"]
descB     [type = "tiled_matrix_desc_t" hidden = on default = "*((tiled_matrix_desc_t*)dataB)"]
beta      [type = "double"]
dataC     [type = "dague_ddesc_t *"]
descC     [type = "tiled_matrix_desc_t" hidden = on default = "*((tiled_matrix_desc_t*)dataC)"]


zsyr2k_24(n, k)
  /* Execution Space */
  n = 0..(descC.nt-1)
  k = 0..(descA.mt-1)

  /* Locality */
  : dataC(n,n)

  READ  A    <- A zsyr2k_24_in_data_A0(n, k)
  READ  B    <- B zsyr2k_24_in_data_B1(n, k)
  RW    C    <- ((k>=1)) ? C zsyr2k_24(n, k-1)
             <- ((0==k)) ? dataC(n,n)
             -> ((descA.mt==(1+k))) ? dataC(n,n)
             -> ((descA.mt>=(2+k))) ? C zsyr2k_24(n, k+1)

BODY
{
  int tempnn = ((n)==(descC.nt-1)) ? (descC.n-(n*descC.nb)) : descC.nb;
  int tempkm = ((k)==(descA.mt-1)) ? (descA.m-(k*descA.mb)) : descA.mb;
  int ldak = BLKLDD( descA, k );
  int ldbk = BLKLDD( descB, k );
  double dbeta = ((k)==(0)) ? (beta) : (1.000000);
  int ldcn = BLKLDD( descC, n );

  printlog("CORE_zsyr2k(%d, %d)\n"
           "\t(uplo, trans, tempnn, tempkm, alpha, A(%d,%d)[%p], ldak, B(%d,%d)[%p], ldbk, dbeta, C(%d,%d)[%p], ldcn)\n",
  n, k, k, n, A, k, n, B, n, n, C);
  DRYRUN(

    CORE_zsyr2k(uplo, trans, tempnn, 
	tempkm, alpha, A /* dataA(k,n) */, 
	ldak, B /* dataB(k,n) */, ldbk, 
	dbeta, C /* dataC(n,n) */, ldcn );
  );

}
END

zsyr2k_24_in_data_B1(n, k) [profile = off]
  /* Execution Space */
  n = 0..(descC.nt-1)
  k = 0..(descA.mt-1)

  /* Locality */
  : dataB(k,n)

  READ  B    <- dataB(k,n)
             -> B zsyr2k_24(n, k)

BODY
{
    /* nothing */
}
END

