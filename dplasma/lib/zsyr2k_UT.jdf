zgemm_34(n, m, k1)
  /* Execution Space */
  n = 0..inline_c %{ return dague_imin((descC.nt-1),(descC.mt-2)); %}
  m = (n+1)..(descC.mt-1)
  k1 = 0..(descA.mt-1)

  /* Locality */
  : dataC(n,m)

  READ  G    <- G zgemm_34_in_data_B0(n, m, k1)
  READ  H    <- H zgemm_34_in_data_A1(n, m, k1)
  RW    J    <- F zgemm_33(n, m, k1)
             -> ((descA.mt==(k1+1))) ? dataC(n,m)
             -> ((descA.mt>=(k1+2))) ? F zgemm_33(n, m, k1+1)

BODY
{
  int tempnn = ((n)==(descC.nt-1)) ? (descC.n-(n*descC.nb)) : descC.nb;
  int tempmm = ((m)==(descC.mt-1)) ? (descC.m-(m*descC.mb)) : descC.mb;
  int tempkm = ((k1)==(descA.mt-1)) ? (descA.m-(k1*descA.mb)) : descA.mb;
  int ldbk = BLKLDD( descB, k1 );
  int ldak = BLKLDD( descA, k1 );
  int ldcn = BLKLDD( descC, n );

  printlog("CORE_zgemm(%d, %d, %d)\n"
           "\t(trans, PlasmaNoTrans, tempnn, tempmm, tempkm, alpha, B(%d,%d)[%p], ldbk, A(%d,%d)[%p], ldak, 1.000000, C(%d,%d)[%p], ldcn)\n",
  n, m, k1, k1, n, G, k1, m, H, n, m, J);
  DRYRUN(

    CORE_zgemm(trans, PlasmaNoTrans, tempnn, 
	tempmm, tempkm, alpha, 
	G /* dataB(k1,n) */, ldbk, H /* dataA(k1,m) */, 
	ldak, 1.000000, J /* dataC(n,m) */, 
	ldcn );
  );

}
END

