zgemm_33(n, m, k1)
  /* Execution Space */
  n = 0..inline_c %{ return dague_imin((descC.nt-1),(descC.mt-2)); %}
  m = (n+1)..(descC.mt-1)
  k1 = 0..(descA.mt-1)

  /* Locality */
  : dataC(n,m)

  READ  D    <- D zgemm_33_in_data_A0(n, m, k1)
  READ  E    <- E zgemm_33_in_data_B1(n, m, k1)
  RW    F    <- ((0==k1)) ? dataC(n,m)
             <- ((k1>=1)) ? J zgemm_34(n, m, k1-1)
             -> J zgemm_34(n, m, k1)

BODY
{
  int tempnn = ((n)==(descC.nt-1)) ? (descC.n-(n*descC.nb)) : descC.nb;
  int tempmm = ((m)==(descC.mt-1)) ? (descC.m-(m*descC.mb)) : descC.mb;
  int tempkm = ((k1)==(descA.mt-1)) ? (descA.m-(k1*descA.mb)) : descA.mb;
  int ldak = BLKLDD( descA, k1 );
  int ldbk = BLKLDD( descB, k1 );
  dague_complex64_t zbeta = ((k1)==(0)) ? ((dague_complex64_t)beta) : (1.000000);
  int ldcn = BLKLDD( descC, n );

  printlog("CORE_zgemm(%d, %d, %d)\n"
           "\t(trans, PlasmaNoTrans, tempnn, tempmm, tempkm, alpha, A(%d,%d)[%p], ldak, B(%d,%d)[%p], ldbk, zbeta, C(%d,%d)[%p], ldcn)\n",
  n, m, k1, k1, n, D, k1, m, E, n, m, F);
  DRYRUN(

    CORE_zgemm(trans, PlasmaNoTrans, tempnn, 
	tempmm, tempkm, alpha, 
	D /* dataA(k1,n) */, ldak, E /* dataB(k1,m) */, 
	ldbk, zbeta, F /* dataC(n,m) */, 
	ldcn );
  );

}
END

zgemm_33_in_data_B1(n, m, k1) [profile = off]
  /* Execution Space */
  n = 0..inline_c %{ return dague_imin((descC.nt-1),(descC.mt-2)); %}
  m = (n+1)..(descC.mt-1)
  k1 = 0..(descA.mt-1)

  /* Locality */
  : dataB(k1,m)

  READ  E    <- dataB(k1,m)
